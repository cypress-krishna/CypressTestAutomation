{
  "name": "cypress-krishna",
  "version": "1.0.0",
  "description": "Cypress Project Creation",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-execution": "npx cypress run --browser firefox --headed",
    "test-execution-chrome": "npx cypress run --browser chrome --headed --spec cypress/e2e/cypress-pageobjects/*.js",
    "run-single-test": "npx cypress run --browser chrome --headed --spec cypress/e2e/screenshots.cy.js",
    "allure:test": "npx cypress run --browser chrome --headed --spec cypress/e2e/cypress-pageobjects/*.js --reporter mocha-allure-reporter",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open  allure-report",
    "cypress-dashboard-run": "npx cypress run --browser chrome --headed --record --key 7faf814a-3247-4dde-9d02-ac2f9cbfca0b"
  },
  "author": "Krishna",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.34.0",
    "allure-commandline": "^2.20.1",
    "cypress": "10.10",
    "cypress-file-upload": "^5.0.8",
    "cypress-mochawesome-reporter": "^3.2.3",
    "mocha-allure-reporter": "^1.4.0"
  }
}
